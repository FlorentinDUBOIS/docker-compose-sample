florentin-dubois.fr, www.florentin-dubois.fr {
    tls server@florentin-dubois.fr
    proxy / portfolio
    gzip
}

personnal.florentin-dubois.fr {
    tls server@florentin-dubois.fr

    root /srv
    basicauth / <username> <password>
    hugo

    proxy /sickrage sickrage:8081 {
        websocket
    }

    proxy /couchpotato couchpotato:5050 {
        websocket
    }

    proxy /deluge deluge:8112 {
        websocket
    }

    proxy /owncloud owncloud {
        websocket
    }

    proxy /rainloop rainloop {
        websocket
    }

    gzip
}

beta.florentin-dubois.fr {
    tls server@florentin-dubois.fr

    basicauth / <username> <password> 
   
    proxy /store store {
        websocket
    }

    proxy /steambot steambot {
        websocket	
    }

    proxy /data phpmyadmin {
	websocket
    } 

    gzip
}

