florentin-dubois.fr, www.florentin-dubois.fr {
  tls server@florentin-dubois.fr

  root /srv/http/portfolio
  log / stdout "{common}"
  gzip

  git github.com/FlorentinDUBOIS/portfolio /srv/http/portfolio {
    branch v7
  }
}

