    tls off
    proxy / portfolio
    gzip
}

personnal.florentin-dubois.fr:80 {
    tls off

    root /srv
    basicauth / <username> <password>
    hugo

    proxy /sickrage sickrage:8081 {
        websocket
    }

    proxy /couchpotato couchpotato:5050 {
        websocket
    }

    proxy /deluge deluge:8112 {
        websocket
    }

    proxy /owncloud owncloud {
        websocket
    }

    proxy /rainloop rainloop {
        websocket
    }

    gzip
}

